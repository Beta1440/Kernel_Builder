on early-init

#set max GPU frequency
write /sys/kernel/tegra_gpu/gpu_cap_rate 756000000
write /sys/kernel/tegra_gpu/gpu_cap_state 1

on boot
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 204000
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq 204000
    
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 2295000
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 2295000

    write /sys/devices/system/cpu/cpuquiet/tegra_cpuquiet/enable 1
    write /sys/devices/system/cpu/cpuquiet/current_governor userspace

    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor sublime
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor sublime
    restorecon_recursive /sys/devices/system/cpu/cpufreq/sublime

    write /sys/block/mmcblk0/queue/read_ahead_kb 1536

    write /sys/block/DEV/queue/scheduler bfq

# KSM options
    write /sys/kernel/mm/ksm/sleep_millisecs 1000
    write /sys/kernel/mm/ksm/pages_to_scan 256
    write /sys/kernel/mm/ksm/deferred_timer 1
    write /sys/kernel/mm/ksm/run 1

